               xmlns:mvvm="clr-namespace:XLabs.Forms.Mvvm;assembly=XLabs.Forms"
               x:Class="HelloWorld.Pages.HomePage">
    <StackLayout Orientation="Vertical">
        <Label Text="{Binding DisplayNews.Title}" />
        <Button Text="{Binding BtnButton.Text}" Command="{Binding MyCommand}"/>
    </StackLayout>
</mvvm:BaseView>